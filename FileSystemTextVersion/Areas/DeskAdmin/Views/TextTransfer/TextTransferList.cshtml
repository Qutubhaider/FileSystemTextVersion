@model List<FileSystemBAL.TextTransferRecord.Models.TextTransferRecord>
@using FileSystemBAL.TextTransferRecord.Models
@{
    Layout = "_LayoutDeskAdmin";
    ViewBag.Title = "My Assign File";
}

@if(Model.Count>0)
{
    foreach(TextTransferRecord record in Model)
    {
        <p>
            @record.inTextTransferRecordId
        </p>
        <br/>
    }
}
else
{
    <p>
        No Record found.
    </p>
}
